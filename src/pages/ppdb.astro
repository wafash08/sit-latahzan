---
import Layout from "@/layouts/Layout.astro";
import CtaButtton from "@/components/cta-buttton.astro";
import { ppdbMetaData } from "@/data/meta-data";
import { createWhatsappMessage, picSd, picSmp, picTk } from "@/data/ppdb-pics";

const ppdbTk = createWhatsappMessage({
  message: picTk.message,
  phone: picTk.phone,
});
